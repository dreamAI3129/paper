"use strict";(function(){function getUsersCountryByIP(){var IP_API_KEY=window.IP_API_KEY;if(!IP_API_KEY)return console.log("API Key not found."),Promise.resolve();return fetch("https://pro.ip-api.com/json/?key="+IP_API_KEY+"&fields=status,message,country,countryCode,continent").then(function(response){return response.json()}).then(function(response){return!("success"!==response.status)&&{country:response.country,continent:response.continent}})}function initCountryLogic(){return new Promise(function(resolve){return getCookie("geoLocation")&&getCookie("geoVersionV2")?void resolve():getUsersCountryByIP().then(function(data){data&&data.country&&(setCookie("geoLocation",data.country,1),setCookie("geoContinent",data.continent,1),setCookie("geoVersionV2","true",1))}).then(function(){return resolve()})})}
window.getCountry=function(){return initCountryLogic().then(function(){return getCookie("geoLocation")})},window.getContinent=function(){return initCountryLogic().then(function(){return getCookie("geoContinent")})}})()